    "gulp-cli": "^2.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.1",
    "nhsuk-frontend": "^0.8.0",
    "nunjucks": "^3.1.7",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "concurrently": "^4.1.0",
    "highlight.js": "^9.14.2",
